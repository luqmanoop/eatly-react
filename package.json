{
  "name": "eatly-react",
  "version": "1.0.0",
  "description": "A food delivery service app for a (fictional) restaurant built using ReactJS",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.prod.js && babel server.js -o dist/server.js",
    "coverage": "jest --coverage",
    "heroku-postbuild": "npm run build",
    "start": "node dist/server.js",
    "start:dev": "webpack-dev-server --config webpack.dev.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeshifu/stark-foods.git"
  },
  "keywords": [],
  "author": "Luqman Olushi <olushilukmon03@gmail.com> (https://codeshifu.github.io/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/codeshifu/stark-foods/issues"
  },
  "homepage": "https://github.com/codeshifu/stark-foods#readme",
  "dependencies": {
    "axios": "0.18.0",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "prop-types": "15.6.2",
    "query-string": "6.2.0",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-redux": "6.0.0",
    "react-router-dom": "4.3.1",
    "redux": "4.0.1",
    "redux-form": "8.1.0",
    "redux-thunk": "2.3.0",
    "slugify": "1.3.4",
    "toastr": "2.1.4"
  },
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-react": "7.0.0",
    "axios-mock-adapter": "1.16.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.1",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "clean-webpack-plugin": "1.0.0",
    "css-loader": "2.0.2",
    "eslint": "5.3.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.3",
    "file-loader": "3.0.1",
    "history": "4.7.2",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.6.0",
    "jest-dom": "3.0.0",
    "node-sass": "4.11.0",
    "react-testing-library": "5.4.4",
    "regenerator-runtime": "0.13.1",
    "sass-loader": "7.1.0",
    "style-loader": "0.23.1",
    "webpack": "4.28.2",
    "webpack-cli": "3.1.2",
    "webpack-dev-server": "3.1.14",
    "webpack-merge": "4.2.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>src/utils/setupTests.js",
    "collectCoverageFrom": [
      "<rootDir>/src/**/*.{js,jsx}",
      "<rootDir>/__tests__/**/*.{js,jsx}",
      "!<rootDir>/src/index.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
